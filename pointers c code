#include <stdio.h>

//adding void function using pointers
void update(int *a,int *b) {
    printf("%d\n",*a+*b);
    printf("%d\n",*a-*b);   
}
//pointers is represented or declared with a asterisk "*"
int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
